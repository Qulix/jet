{
  "compilerOptions": {
    "target": "es2015",
    "lib": [
      "es2015"
    ],
    "allowJs": false,
    "module": "es2015",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "pretty": true,
    "noEmit": true
  },
  "exclude": [
    "node_modules"
  ]
}
